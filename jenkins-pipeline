pipeline{
    agent any
    
    tools {
  maven 'maven3'
}

    stages{
        stage ("Git checkout"){
        steps{
           git 'https://github.com/skwandoodi/Jenkins-Full-Course-on-aws-Linux.git' 
        }
        }
        stage("Maven version Stage"){
        steps{
        sh 'mvn -version'
        }
        }
        stage("maven clean stage"){
        steps{
        sh 'mvn (\'clean\', \'Maven/pom.xml\')'
        }
    }
}
